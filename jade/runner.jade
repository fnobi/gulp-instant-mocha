doctype html
html
  head
    meta(charset="utf-8")
    title mocha test
  body
    #mocha
    script(src=mochaPath)
    script!= setupCode
    if assertPath
      script(src=assertPath)
    script!= testCode
    if useCheckLeaks
      script.
        mocha.checkLeaks();
    script.
      mocha.run();


